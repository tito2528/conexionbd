	<queryString>
		<![CDATA[SELECT 
    u.id_usuario,
    u.usu_usuario,
    u.usu_nombre,
    u.usu_apellido,
    r.rol_nombre,
    r.rol_descripcion,
    u.estado
FROM 
    usuario u
INNER JOIN 
    rol r ON u.id_rol = r.id_rol
ORDER BY 
    u.id_usuario;
]]>
